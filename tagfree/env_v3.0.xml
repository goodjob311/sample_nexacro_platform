	<Edit>

		<!-- 템플릿 설정 파일의 경로를 지정해줍니다. -->
		<Template>http://www.tagfree.com/demo_files/Templates.xml</Template>
	
		
		<!-- HTML charset(설정 가능 항목 : http://www.iana.org/assignments/character-sets)ks_c_5601-1987-->
		<Charset apply="true">utf-8</Charset>
		<!-- 작성하는 HTML 문서의 제목 -->
		<Title apply="false">TAGFREE Active Designer</Title>
		<!-- HTML 디자인 및 편집 가능 여부 < on : 편집 가능, off : 편집 불가 > -->
		<EditMode apply="false">on</EditMode>
		<!-- HTML 디자인 탭에서의 편집 시 탭 키에 대한 공백 문자열 개수 지정 -->
		<TabSize apply="false">4</TabSize>
		<!-- HTML 디자인 탭에서의 편집 시 문서의 초기 백그라운드 색상 지정 < 16진수로 지정, ex)#ffffff > -->
		<BgColor apply="false">#00FFFF</BgColor>
		<!-- HTML 디자인 탭에서의 편집 시 문서의 초기 백그라운드 이미지 지정 : URL로 지정함. -->
		<BgImage apply="false"/>
		<!-- HTML 디자인 탭에서의 편집 시 초기 폰트 색상 지정 < 16진수로 지정, ex)#ffffff > -->
		<FontColor apply="false">#00ff00</FontColor>
		<!-- HTML 디자인 탭에서의 편집 시 초기 폰트 종류 지정 -->
		<FontFamily apply="true">gulim</FontFamily>

		<!-- Font Combo Box 의 값을 설정.  -->
		<!-- apply 가 true 일 경우 설정 된 폰트 값들이 나타남.  -->
		<!-- apply 가 false 일 경우 사용자 PC 에 설치 된 Font 들이 나타남. -->
		<!-- 설정 된 Font 중 사용자의 PC 에 설치 되어 있지 않는 Font 는 나타나지 않음 -->
		<FontFamilyGroup apply="false">
			<Font>바탕</Font>
			<Font>바탕체</Font>
			<Font>굴림</Font>
			<Font>굴림체</Font>
			<Font>돋움</Font>
			<Font>돋움체</Font>
			<Font>궁서</Font>
			<Font>궁서체</Font>
			<Font>맑은 고딕</Font>
			<Font>Verdana</Font>
			<Font>새굴림</Font>
		</FontFamilyGroup>

		<!-- HTML 디자인 탭에서의 편집 시 초기 폰트 크기 지정, ex)12pt, 12px -->
		<FontSize apply="true">9pt</FontSize>
		<!-- Grid 선택 시 Grid의 색상 < 16 진수로 지정, ex)#ffffff > -->
		<GridColor apply="true">#000000</GridColor>
		<!-- Grid 선택 시 Grid 선의 타입 < line : 선, dot : 점선 > -->
		<GridShape apply="false">dot</GridShape>
		<!-- Grid 선택 시 Grid 가로 격자 간격 : 픽셀로 지정 -->
		<GridSize apply="true">100</GridSize>
		<!-- Grid 선택 시 Grid 세로 격자 간격 : 픽셀로 지정 -->
		<GridHeightSize apply="true">200</GridHeightSize>
		<!-- Grid 선택 시 Grid 가로 여백 : 픽셀로 지정 -->
		<GridMarginSize apply="false">50</GridMarginSize>
		<!-- Grid 선택 시 Grid 세로 여백 : 픽셀로 지정 -->
		<GridHeightMarginSize apply="false">10</GridHeightMarginSize>
		<!-- Snap 옵션 선택 시 Snap 크기 설정 : 픽셀로 지정, Grid 격자 간격의 배수로 설정하는 것이 유용 -->
		<SnapSize apply="false">50</SnapSize>
		<!-- 테이블의 삽입, 드로잉시에 적용될 테이블의 기본적인 속성 지정 -->
		<!-- <Table>태그에 속성을 지정하는 방식과 동일하게 속성과 속성값을 지정(대소문자 구분) -->
		<!-- 지정 가능한 속성 : background, bgColor, border, borderColor, borderColorDark, borderColorLight, cellPadding, cellSpacing -->
		<TableStyle apply="true" background="" bgColor="white" border="1" borderColor="#6E6E6E" borderColorDark="" borderColorLight="" cellPadding="0" cellSpacing="0" align="left" style="BORDER-COLLAPSE: collapse;BORDER-RIGHT: medium none; BORDER-TOP: medium none;BORDER-BOTTOM: medium none; BORDER-LEFT: medium none;"/>
		<!-- <TD>태그에 속성을 지정하는 방식과 동일하게 속성과 속성값을 지정(대소문자 구분) -->
		<TDStyle apply="true" bgColor="" borderColor="" align="" noWrap="" style="BORDER-RIGHT: #000000 1pt solid; BORDER-TOP: #000000 1pt solid; BORDER-LEFT: #000000 1pt solid; BORDER-BOTTOM: #000000 1pt solid"/>		
		<!--외부스타일 시트 링크-->
		<StyleSheetLink apply="false" href="http://www.tagfree.com/tagfree.css"/>
		<!--첨부파일들의 한도 사이즈, 단위 byte , 무제한 : apply=false 또는 값 -1(음수값)-->
		<AttachmentLimit apply="false">1048576</AttachmentLimit>
		<!--Base태그 표현 설정 < true : 안보임, false : 보임 ,defalut:false > -->
		<HideBaseElement apply="false">false</HideBaseElement>		
		<!-- Image Max Size < limit : 한계 기준, fixed : 기준 초과시 지정될 사이즈 > -->		
		<ImageSize apply="false" limit="400" fixed="500"/>		
		<!-- Image Resizing 비율 처리 여부 < true : 비율처리, false : 일반처리 ,defalut:false > -->
		<ImageResizingRate apply="false">true</ImageResizingRate>		
		<!-- Attach 관련 설정 -->		
		<!-- 첨부 기능 , class : 첨부태그 클래스명, style : 첨부태그 스타일, src : 기본 이미지, drop : 드래그앤드랍 첨부(true,false), paste : 붙여넣기 첨부(true,false), position : 첨부 위치(any,top) -->
		<!-- name의 형식 : ".확장자" -->
		<Attachment apply="false" class="twe_attach" style="font-size:9pt;" src="http://tagfree.com/icon/blank.gif" drop="false" paste="false" position="any">
			<Extension name=".xls">http://tagfree.com/icon/xls.gif</Extension>
			<Extension name=".doc">http://tagfree.com/icon/doc.gif</Extension>
			<Extension name=".txt">http://tagfree.com/icon/txt.gif</Extension>
			<Extension name=".pdf">http://tagfree.com/icon/pdf.gif</Extension>
			<Extension name=".hwp">http://tagfree.com/icon/hwp.gif</Extension>
			<Extension name=".ppt">http://tagfree.com/icon/ppt.gif</Extension>
			<Extension name=".avi">http://tagfree.com/icon/media.gif</Extension>
			<Extension name=".mp3">http://tagfree.com/icon/media.gif</Extension>
			<Extension name=".wma">http://tagfree.com/icon/media.gif</Extension>
			<Extension name=".wmv">http://tagfree.com/icon/media.gif</Extension>
			<Extension name=".asf">http://tagfree.com/icon/media.gif</Extension>
			<Extension name=".mpg">http://tagfree.com/icon/media.gif</Extension>
		</Attachment>
		<!-- DefaultStyleCSS 기본 스타일시트를 설정한다. < true : 적용, false : 미적용 > -->
		<DefaultStyleCSS apply="false">			
			P {
				MARGIN-TOP: 0pt; MARGIN-BOTTOM: 0pt
			}
			BLOCKQUOTE {
				MARGIN-TOP: 0pt; MARGIN-BOTTOM: 0pt
			}
			FONT {
				FONT-SIZE: 10pt
			}
			TD {
				WORD-BREAK: break-all
			}
		</DefaultStyleCSS>
		<!-- AutoSave 자동 저장 기능를 설정한다. < true : 적용, false : 미적용 > -->
		<!-- initDialog : 에디터가 실행될 때 다이얼로그를 설정한다. < on : 적용, off : 미적용 > -->
		<!-- 분 단위로 타임 설정 ex) 1, 10 -->
		<AutoSave apply="true" initDialog="off">1</AutoSave>
		<DelayTime apply="true">1000</DelayTime>
		<!-- 이미지, 플래시, 동영상 삽입시 파일을 체크한다. 파일의 확장자를 작성해줘야함.-->
		<!-- jpg, gif, bmp, png, swf, avi, mpg 는 기본적으로 허용되며, 추가 해주신 파일만 허용됨. 소문자로 작성.-->
		<InsertBinaryAcception apply="true">
			<Pattern>asf</Pattern>
			<Pattern>mpeg</Pattern>
			<Pattern>jpeg</Pattern>
			<!--<Pattern></Pattern>-->
			<!--<Pattern></Pattern>-->
		</InsertBinaryAcception>
		<!-- 에디터 기능 중 드레그&드롭 기능을 제어합니다. <true : 사용 안함 , false : 사용 > -->
		<DragDrop apply="false"/>
		<!-- 에디터 기능 중 붙여 넣기 기능을 제어합니다. <true : 사용 안함 , false : 사용 > -->
		<PasteEable apply="false"/>
		<!-- 테이블에서 tab키의 기능을  제어합니다. <true : 글자 뛰음 , false : 기본동작 > -->
		<TabInTable apply="true"/>
	</Edit>
	<Mime>
		<!-- HTML 인코딩 시에 사용할 인코딩 타입 설정 < base64 : BASE64 인코딩, qp : Quoted-Printable 인코딩 > -->
		<HtmlEncodingType>base64</HtmlEncodingType>
		<!-- HTML 인코딩 시에 인코딩 할 리소스 파일의 종류 설정 < img : img 태그, bgsound : body 태그의 bgsound 속성 > -->
		<!-- 추가 : anchor는 a태그의 href를 업로드, include의 link, script 업로드 -->
		<!--<EncodingFileType>image, bgsound, multimedia, anchor, link, script</EncodingFileType>-->
		<EncodingFileType>image, bgsound, multimedia</EncodingFileType>
		<!-- 마임값으로 생성할 파일의 범위 < all : 로컬파일+원격파일, local : 로컬파일, 기본값 : 로컬파일 > -->
		<EncodingFileRange showProgress="false">local</EncodingFileRange>
		<!-- 마임값으로 생성할 파일중 허용될 대상 -->
		<EncodingFileAcception apply="false">
			<Pattern>.jsp</Pattern>
			<!--<Pattern>http://localhost</Pattern>-->
			<!--<Pattern>file://c:/</Pattern>-->
			<!--<Pattern>file:///c:/</Pattern>-->
			<!--<Pattern>file:///C:/</Pattern>-->
		</EncodingFileAcception>
		<!-- 마임값으로 생성할 파일중 제외될 대상 -->
		<EncodingFileException>
			<Pattern>http://tagfree.com/upload</Pattern>
			<Pattern>http://localhost/object.asp</Pattern>
			<Pattern>http://tagfree.com/icon</Pattern>
			<!--<Pattern>.bmp</Pattern>-->
			<!--<Pattern>file://c:/</Pattern>-->
			<!--<Pattern>file:///c:/</Pattern>-->
			<!--<Pattern>file:///C:/</Pattern>-->
		</EncodingFileException>

